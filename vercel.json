{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/user",
      "dest": "/",
    }
  ],
  {
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
}
